{
  "name": "gulp-transform",
  "version": "1.0.4",
  "description": "Gulp plugin for performing arbitrary transformations on the contents of files.",
  "main": "index.js",
  "files": [
    "index.js",
    "LICENSE.txt",
    "README.md"
  ],
  "scripts": {
    "test": "mocha",
    "clean": "rm -rf ./node_modules ./coverage",
    "coverage": "istanbul cover _mocha && open ./coverage/lcov-report/index.html",
    "coveralls": "istanbul cover _mocha && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage"
  },
  "engines": {
    "node": ">=0.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akim-mcmath/gulp-transform.git"
  },
  "keywords": [
    "gulpplugin",
    "gulp",
    "transform",
    "replace",
    "wrapper",
    "wrap"
  ],
  "author": "Akim McMath <akim.elijah.mcmath@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/akim-mcmath/gulp-transform/issues"
  },
  "homepage": "https://github.com/akim-mcmath/gulp-transform#readme",
  "peerDependencies": {
    "gulp": "^3.0.0"
  },
  "dependencies": {
    "gulp-util": "^3.0.7"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cheerio": "^0.20.0",
    "coveralls": "^2.11.6",
    "event-stream": "^3.3.2",
    "gulp": "^3.9.1",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.1",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0"
  }
}
